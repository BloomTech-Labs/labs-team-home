{
	"name": "frontend",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"react": "^16.7.0",
		"react-dom": "^16.7.0",
		"react-redux": "^6.0.0",
		"react-scripts": "2.1.3",
		"styled-components": "^4.1.3"
	},
	"devDependencies": {
		"eslint-config-prettier": "^3.1.0",
		"eslint-plugin-prettier": "^3.0.0",
		"husky": "^1.1.3",
		"prettier": "^1.15.1",
		"pretty-quick": "^1.8.0"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged --verbose"
		}
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"plugin:prettier/recommended"
		]
	},
	"prettier": {
		"singleQuote": true,
		"useTabs": true,
		"endOfLine": "lf"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
}
